{
  "engines": {
    "yarn": "^1.21.1"
  },
  "name": "gatsby-starter-mdx-typescript",
  "version": "1.0.0",
  "description": "A Gatsby starter using MDX and Typescript",
  "main": "index.js",
  "repository": "git@github.com:hankandre/gatsby-starter-mdx-typescript.git",
  "bugs": "https://github.com/hankandre/gatsby-starter-mdx-typescript/issues",
  "keywords": [
    "gatsby",
    "typescript",
    "emotion",
    "starter",
    "mdx",
    "jest",
    "testing",
    "eslint"
  ],
  "author": "Hank Andre <dunordic@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "start": "gatsby start",
    "build": "gatsby build",
    "test": "jest --config jest-configs/jest-test.config.js",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/mdx": "^1.5.3",
    "@mdx-js/react": "^1.5.3",
    "@mdx-js/tag": "^0.20.3",
    "emotion-theming": "^10.0.27",
    "gatsby": "^2.18.21",
    "gatsby-image": "^2.2.38",
    "gatsby-plugin-emotion": "^4.1.21",
    "gatsby-plugin-feed": "^2.3.26",
    "gatsby-plugin-google-analytics": "^2.1.32",
    "gatsby-plugin-manifest": "^2.2.37",
    "gatsby-plugin-mdx": "^1.0.67",
    "gatsby-plugin-offline": "^3.0.31",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sharp": "^2.3.13",
    "gatsby-plugin-transition-link": "^1.17.7",
