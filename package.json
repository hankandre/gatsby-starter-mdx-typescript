    "gatsby-remark-smartypants": "^2.1.20",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-remark": "^2.6.48",
    "gatsby-transformer-sharp": "^2.3.12",
    "gsap": "^3.0.5",
    "prism-react-renderer": "^1.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-live": "^2.2.2",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-react": "^7.8.0",
    "@babel/preset-typescript": "^7.8.0",
    "@testing-library/dom": "^6.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.3",
    "@types/react": "^16.9.17",
    "@types/typography": "^0.16.3",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.15.0",
    "babel-jest": "^24.9.0",
    "babel-preset-gatsby": "^0.2.27",
    "eslint": "^6.8.0",
    "eslint-config-kentcdodds": "^14.7.0",
    "eslint-plugin-jest": "^23.6.0",
    "husky": "^4.0.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-chain": "^1.1.2",
    "jest-extended": "^0.11.2",
    "jest-runner-eslint": "^0.7.5",
    "jest-validate": "^24.9.0",
    "jest-watch-continue": "^1.1.4",
    "jest-watch-master": "^1.0.0",
    "jest-watch-select-projects": "^1.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "jest --config jest-configs/jest-test.config.js --bail --findRelatedTests",
      "eslint . --ext .js,.jsx,.tsx,.ts --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
